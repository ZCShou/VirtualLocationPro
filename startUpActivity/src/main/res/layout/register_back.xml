<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="32dip"
    android:layout_height="32dip" >

    <item android:drawable="@drawable/titlebar_btn_back_press" android:state_pressed="true"
        android:layout_width="32dip"
        android:layout_height="32dip" />
    <!-- pressed -->
    <item android:drawable="@drawable/titlebar_btn_back_press" android:state_focused="true"
        android:layout_width="32dip"
        android:layout_height="32dip" />
    <!-- focused -->
    <item android:drawable="@drawable/titlebar_btn_back"
        android:layout_width="32dip"
        android:layout_height="32dip" />
    <!-- default -->
</selector>